{
  "name": "aura-helper-sfdx",
  "displayName": "Aura Helper SFDX",
  "description": "Aura Helper CLI aplication Tools converted into SFDX Plugin",
  "version": "0.0.1",
  "author": "Juan José Longoria López",
  "bugs": "https://github.com/JJLongoria/aura-helper-sfdx/issues",
  "dependencies": {
    "@aurahelper/connector": "file: ./../../../NodeJS/Aura Helper Framework/ah-connector",
    "@aurahelper/core": "file: ./../../../NodeJS/Aura Helper Framework/ah-core",
    "@aurahelper/dependencies-manager": "file: ./../../../NodeJS/Aura Helper Framework/ah-dependencies-manager",
    "@aurahelper/git-manager": "file: ./../../../NodeJS/Aura Helper Framework/ah-git-manager",
    "@aurahelper/ignore": "file: ./../../../NodeJS/Aura Helper Framework/ah-ignore",
    "@aurahelper/metadata-factory": "file: ./../../../NodeJS/Aura Helper Framework/ah-metadata-factory",
    "@aurahelper/package-generator": "file: ./../../../NodeJS/Aura Helper Framework/ah-package-generator",
    "@aurahelper/xml-compressor": "file: ./../../../NodeJS/Aura Helper Framework/ah-xml-compressor",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^2.0.0",
    "@salesforce/core": "^2",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "^2",
    "@salesforce/dev-scripts": "^0",
    "@salesforce/prettier-config": "^0",
    "@salesforce/ts-sinon": "^1",
    "@types/chai": "^4",
    "@types/jsforce": "^1.9.29",
    "@types/mocha": "^8",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "chai": "^4",
    "eslint": "^7",
    "eslint-config-oclif": "^3.1",
    "eslint-config-prettier": "^8",
    "eslint-config-salesforce": "^0",
    "eslint-config-salesforce-typescript": "^0",
    "eslint-plugin-header": "^3",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsdoc": "^35",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-typescript": "^0",
    "globby": "^11",
    "husky": "^4",
    "mocha": "^8",
    "nyc": "^15",
    "prettier": "^2",
    "pretty-quick": "^3",
    "rimraf": "^3.0.2",
    "sinon": "10.0.0",
    "ts-node": "^10",
    "typescript": "4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/JJLongoria/aura-helper-sfdx",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "GPL-3.0-only",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/JJLongoria/aura-helper-sfdx",
  "scripts": {
    "build": "tsc -p .",
    "watch": "tsc -p . --watch",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint src/**/*.ts test/**/*.ts",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts --require ts-node/register mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
