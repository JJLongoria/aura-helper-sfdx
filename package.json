{
  "name": "sfdx-aura-helper",
  "publisher": "Kanko",
  "displayName": "Aura Helper SFDX",
  "description": "Aura Helper CLI tool converted into SFDX Plugin",
  "version": "1.0.0",
  "author": "Juan José Longoria López",
  "bugs": "https://github.com/JJLongoria/sfdx-aura-helper/issues",
  "dependencies": {},
  "devDependencies": {},
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/JJLongoria/sfdx-aura-helper.git",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "GPLv3",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "hello": {
        "description": "Commands to say hello."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/JJLongoria/sfdx-aura-helper.git",
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint src/**/*.ts test/**/*.ts",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts --require ts-node/register mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
